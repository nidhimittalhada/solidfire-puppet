{
  "name": "solidfire-solidfire",
  "version": "0.0.1",
  "author": "solidfire",
  "summary": "Module for managing a SolidFire cluster.",
  "license": "Apache-2.0",
  "source": "https://github.com/solidfire/solidfire-puppet",
  "project_page": "https://github.com/solidfire/solidfire-puppet",
  "issues_url": "https://github.com/solidfire/solidfire-puppet/issues",
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.7.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6.0",
        "7.0"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7.0",
        "8.0"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.6.0 < 5.0.0"
    }
  ]
}
